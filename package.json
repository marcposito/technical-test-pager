{
  "name": "technical-test-pager",
  "version": "1.0.0",
  "description": "Aircall home assesment",
  "author": "Marc Posito Roda",
  "license": "ISC",
  "engines": {
    "npm": ">= 8.1.2",
    "node": ">= 16.14.0"
  },
  "main": "src/app.ts",
  "scripts": {
    "start": "node ./dist/app.js",
    "start:dev": "npm run ts -- src/app.ts",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "watch": "tsc -w",
    "ts": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_PATH=src node --require ts-node/register --require universal-dotenv/register",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha -r ts-node/register test/unit/**/*.spec.ts",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov npm run test:unit",
    "test:xunit": "mocha -r xunit src/**/*.spec.ts",
    "coverage:cobertura": "NODE_ENV=test nyc --reporter=cobertura npm test"
  },
  "dependencies": {
    "joi": "17.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.205",
    "@swc/wasm": "^1.2.205",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.6",
    "chai-exclude": "^2.1.0",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "universal-dotenv": "^3.2.6"
  }
}
